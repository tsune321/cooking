{
    "builds": [
        {
            "src": "backend/public/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/public/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}